{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Translation File Schema",
  "type": "object",
  "properties": {
    "Angular Momentum": {
      "type": "string",
      "description": "Main page title"
    }
    ,"This project is designed to rapidly spin up Angular applications...": {
      "type": "string",
      "description": "Main page description"
    }
    ,"If you find this project helpful and want to see it grow, consider...": {
      "type": "string",
      "description": "Main page beg for support"
    }
    ,"v.": {
      "type": "string",
      "description": "Main page version"
    }
    ,"Language": {
      "type": "string",
      "description": "Name of the language menu"
    }
    ,"There are {{count}} components.": {
      "type": "string",
      "description": "Message about the number of components, using a pluralization format"
    }
    ,"App Version": {
      "type": "string",
      "description": "Name of the application version component"
    }
    ,"Environment": {
      "type": "string",
      "description": "Name of the environment component"
    }
    ,"Using {{environmentName}} environment.": {
      "type": "string",
      "description": "Message about the environment"
    }
    ,"Development": {
      "type": "string",
      "description": "Environment name for development"
    }
    ,"Production": {
      "type": "string",
      "description": "Environment name for production"
    }
    ,"GraphQL API": {
      "type": "string",
      "description": "API button label"
    }
    ,"IndexedDB": {
      "type": "string",
      "description": "IndexedDB button label"
    }
    ,"Donate to Support": {
      "type": "string",
      "description": "Donate button label"
    }
    ,"Enter some text to have it persist between page reloads": {
      "type": "string",
      "description": "IndexedDB component description"
    }
    ,"Features": {
      "type": "string",
      "description": "Features button label"
    }
    ,"Turn features on and off": {
      "type": "string",
      "description": "Features component description"
    }
  },
  "required": [
    "Angular Momentum"
    ,"This project is designed to rapidly spin up Angular applications..."
    ,"If you find this project helpful and want to see it grow, consider..."
    ,"v."
    ,"Language"
    ,"There are {{count}} components."
    ,"App Version"
    ,"Environment"
    ,"Using {{environmentName}} environment."
    ,"Development"
    ,"Production"
    ,"GraphQL API"
    ,"IndexedDB"
    ,"Features"
    ,"Enter some text to have it persist between page reloads"
    ,"Turn features on and off"
  ],
  "additionalProperties": false
}
