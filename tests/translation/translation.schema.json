{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Translation File Schema",
  "type": "object",
  "properties": {
    "Momentum": {
      "type": "string",
      "description": "Main page title"
    }
    ,"This project is designed to rapidly spin up Angular applications...": {
      "type": "string",
      "description": "Main page description"
    }
    ,"If you find this project helpful and want to see it grow, consider...": {
      "type": "string",
      "description": "Main page beg for support"
    }
    ,"v.": {
      "type": "string",
      "description": "Main page version"
    }
    ,"Language": {
      "type": "string",
      "description": "Name of the language menu"
    }
    ,"Change Log": {
      "type": "string",
      "description": "Title of the change log menu"
    }
    ,"This app is {semver} out of date.": {
      "type": "string",
      "description": "Message about the app version"
    }
    ,"{patches} patch(es)": {
      "type": "string",
      "description": "Message about the number of patches"
    }
    ,"{minors} minor version(s)": {
      "type": "string",
      "description": "Message about the number of minor versions"
    }
    ,"{majors} major release(s)": {
      "type": "string",
      "description": "Message about the number of major releases"
    }
    ,"If it doesn’t update momentarily, please try to clear your cache and refresh your browser.": {
      "type": "string",
      "description": "Direction to clear cache and reload for web users"
    }
    ,"If you encounter problems, you can use the webapp at {url} until an app update is ready.": {
      "type": "string",
      "description": "Direction for app users to use the webapp until an app update is ready"
    }
    ,"Environment": {
      "type": "string",
      "description": "Name of the environment component"
    }
    ,"App Version": {
      "type": "string",
      "description": "Name of the app version component"
    }
    ,"{environmentName} environment": {
      "type": "string",
      "description": "Message about the environment"
    }
    ,"Development": {
      "type": "string",
      "description": "Environment name for development"
    }
    ,"Production": {
      "type": "string",
      "description": "Environment name for production"
    }
    ,"GraphQL API": {
      "type": "string",
      "description": "API button label"
    }
    ,"IndexedDB": {
      "type": "string",
      "description": "IndexedDB button label"
    }
    ,"Installers": {
      "type": "string",
      "description": "Installers button label"
    }
    ,"Install Angular Momentum on your device instead of using it via a web browser:": {
      "type": "string",
      "description": "Installers component description"
    }
    ,"Other Installers": {
      "type": "string",
      "description": "Other Installers header label"
    }
    ,"Support": {
      "type": "string",
      "description": "Donate button label"
    }
    ,"Persistent Text": {
      "type": "string",
      "description": "Persistent Text input label"
    }
    ,"Enter some text to have it persist between page reloads (saved in browser storage)": {
      "type": "string",
      "description": "IndexedDB component description"
    }
    ,"Features": {
      "type": "string",
      "description": "Features button label"
    }
    ,"Turn features on and off for feature flag testing": {
      "type": "string",
      "description": "Features component description"
    }
  },
  "required": [
    "Momentum"
    ,"This project is designed to rapidly spin up Angular applications..."
    ,"If you find this project helpful and want to see it grow, consider..."
    ,"v."
    ,"Language"
    ,"Change Log"
    ,"This app is {semver} out of date."
    ,"{patches} patch(es)"
    ,"{minors} minor version(s)"
    ,"{majors} major release(s)"
    ,"If it doesn’t update momentarily, please try to clear your cache and refresh your browser."
    ,"If you encounter problems, you can use the webapp at {url} until an app update is ready."
    ,"App Version"
    ,"Environment"
    ,"{environmentName} environment"
    ,"Development"
    ,"Production"
    ,"GraphQL API"
    ,"IndexedDB"
    ,"Installers"
    ,"Install Angular Momentum on your device instead of using it via a web browser:"
    ,"Other Installers"
    ,"Features"
    ,"Support"
    ,"Persistent Text"
    ,"Enter some text to have it persist between page reloads (saved in browser storage)"
    ,"Turn features on and off for feature flag testing"
  ],
  "additionalProperties": false
}
