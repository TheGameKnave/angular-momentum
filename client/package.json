{
  "name": "angular-momentum",
  "version": "21.1.12",
  "siteUrl": "https://angularmomentum.app",
  "repository": "https://github.com/TheGameKnave/angular-momentum",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxy.conf.js",
    "build": "ng build && node scripts/prune-flags.js",
    "dev": "NODE_ENV=development npm run start",
    "test": "NODE_ENV=development ng test --configuration test --browsers=ChromeHeadless --watch=false",
    "test:debug": "NODE_ENV=development KARMA_CONSOLE=true ng test --configuration test --browsers=ChromeHeadless --watch=false",
    "watch": "NODE_ENV=development ng test",
    "build-heroku": "ng build --configuration=${NODE_ENV} && node scripts/prune-flags.js",
    "heroku-postbuild": "npm run build-heroku && npx playwright install chromium --with-deps",
    "setLocal": "node ./scripts/set-local-env.js",
    "tauri": "tauri",
    "tauri:dev": "npm run setLocal && tauri dev",
    "tauri:android": "npm run setLocal && tauri android dev",
    "tauri:ios": "npm run setLocal && tauri ios dev",
    "tauri:build": "npm run setLocal && tauri build",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^21.0.2",
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^21.0.2",
    "@angular/cli": "21.0.2",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/compiler-cli": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/platform-server": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@angular/service-worker": "^21.0.3",
    "@angular/ssr": "^21.0.3",
    "@jsverse/transloco": "^8.2.0",
    "@jsverse/transloco-locale": "^8.2.0",
    "@jsverse/transloco-messageformat": "^8.2.0",
    "@jsverse/transloco-persist-lang": "^8.2.0",
    "@primeng/themes": "^21.0.1",
    "@supabase/supabase-js": "^2.86.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-opener": "^2.5.2",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "decancer": "^3.3.3",
    "fast-deep-equal": "^3.1.3",
    "http-proxy-middleware": "^3.0.5",
    "i18n-l10n-flags": "^2.0.0",
    "idb": "^8.0.0",
    "ngx-markdown": "^21.0.1",
    "@types/compression": "^1.8.1",
    "compression": "^1.8.0",
    "ngx-socket-io": "~4.9.3",
    "ngx-turnstile": "^0.2.5",
    "postcss": "^8.5.3",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.1",
    "playwright": "^1.57.0",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin-template": "^21.0.1",
    "@angular/build": "^21.0.2",
    "@eslint/js": "^9.39.1",
    "@tauri-apps/cli": "^2.9.5",
    "@types/jasmine": "~5.1.13",
    "@types/node": "^25.0.3",
    "@types/socket.io": "^3.0.2",
