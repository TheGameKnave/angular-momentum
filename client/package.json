{
  "name": "angular-momentum",
  "version": "0.20.1",
  "scripts": {
    "ng": "ng",
    "start": "node scripts/copy-dev-assets.js && ng serve --hmr --proxy-config src/proxy.conf.json",
    "build": "ng build",
    "test": "node scripts/copy-dev-assets.js && ng test --configuration test --browsers=ChromeHeadless --watch=false",
    "watch": "node scripts/copy-dev-assets.js && ng test",
    "copy-dev-assets": "node scripts/copy-dev-assets.js",
    "build-heroku": "ng build --configuration=${NODE_ENV}",
    "heroku-postbuild": "npm run copy-dev-assets && npm run build-heroku",
    "setLocal": "node ./scripts/set-local-env.js",
    "tauri": "tauri",
    "tauri:dev": "npm run setLocal && tauri dev",
    "tauri:android": "npm run setLocal && tauri android dev",
    "tauri:ios": "npm run setLocal && tauri ios dev",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^20.1.3",
    "@angular/animations": "^20.1.3",
    "@angular/cli": "20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@angular/service-worker": "^20.1.3",
    "@jsverse/transloco": "^7.4.3",
    "@jsverse/transloco-locale": "^7.0.1",
    "@jsverse/transloco-messageformat": "^7.0.1",
    "@jsverse/transloco-persist-lang": "^7.0.1",
    "@tailwindcss/postcss": "^4.1.5",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "fast-deep-equal": "^3.1.3",
    "i18n-l10n-flags": "^2.0.0",
    "idb": "^8.0.0",
    "ngx-cookie-service": "^20.0.1",
    "ngx-markdown": "^20.0.0",
    "ngx-socket-io": "~4.9.3",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.5",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin-template": "^19.8.1",
    "@angular/build": "^20.1.3",
    "@eslint/js": "^9.29.0",
    "@tauri-apps/cli": "^2",
    "@types/jasmine": "~5.1.0",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "angular-eslint": "^19",
    "eslint": "^9.32.0",
    "eslint-plugin-rxjs-x": "^0.7.7",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-sabarivka-reporter": "^3.3.1",
    "postcss": "^8.5.3",
    "typescript": "~5.8.3"
  }
}
